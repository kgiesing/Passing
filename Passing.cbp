<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Passing" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Option show_notes="1">
			<notes>
				<![CDATA[This project demonstrates the various forms of passing and returning objects:
- By value
- By reference
- By pointer ("simulated reference")

It will show what happens when concrete base classes, abstract base classes, and concrete subclasses are passed in these manners.

Output will be displayed on the console.]]>
			</notes>
		</Option>
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/Passing" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
					<Add directory="include" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/Passing" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add directory="include" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-fexceptions" />
		</Compiler>
		<Unit filename="include/ConcreteBase.h" />
		<Unit filename="include/ConcreteDerived.h" />
		<Unit filename="src/ConcreteBase.cpp" />
		<Unit filename="src/ConcreteDerived.cpp" />
		<Unit filename="src/main.cpp" />
		<Extensions>
